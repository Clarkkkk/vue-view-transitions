{
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "never",
    "source.fixAll.eslint": "explicit"
  },
  "vue.codeActions.enabled": false,
  "vue.codeLens.enabled": false,
  "vue-semantic-server.trace.server": "messages",
  "vue-syntactic-server.trace.server": "messages",
  "vue.autoInsert.parentheses": true,
  "volar.takeOverMode.extension": "Vue.volar",
  "vue.complete.normalizeComponentImportName": false,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
